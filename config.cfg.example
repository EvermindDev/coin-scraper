[coinbasecap]
coin_limit = 5
start_page_no = 1

[scrape]
# headless: False, True
headless = False

# Duration of the pause in seconds
time_sleep = 1

retry_limit = 3

[db-data]
# Use database to store data: False, True
use_database = False
host = <host>
user = <user>
password = <password>
database = <database>

[file-data]
# Use file to store data: False, True
use_file = True
data_file_name = coins
data_file_format = csv

# Download coin logo: False, True
download_logo = True

[logs]
log_file_name = main